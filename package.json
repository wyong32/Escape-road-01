{
  "name": "escape-road-project",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev:frontend": "cd Escape-road && vite",
    "build:frontend": "cd Escape-road && npm run build",
    "preview:frontend": "cd Escape-road && npm run preview",
    "lint:frontend": "cd Escape-road && npm run lint",
    "format:frontend": "cd Escape-road && npm run format",
    "dev:backend": "nodemon api/index.js",
    "build": "cd Escape-road && npm install && npm run build",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "dependencies": {
    "@vercel/kv": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "axios": "^1.8.4",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "concurrently": "^8.0.0"
  }
}
